package ara; 

import java.awt.FlowLayout;


public class HomeClass {
	public static void main (String [] arc)
	{
	  //System.out.println("Hello");
		JFrame win; 
		win = new JFrame("Home Window"); // window title given to ctor  
		win.setSize(220, 125);  // size of the window
		win.setVisible(true); // displays frame on screen
		win.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);  // causes app to exit when x clicked
	}
}
