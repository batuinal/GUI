	{

	super(inTitle); setLayout(new FlowLayout());
	dogButton = new add(dogButton);
	catButton = new add(catButton);
	cowButton = new add(cowButton);
	label = new JLabel("Sound: "); add(label);
	}
	public class HomeListener implements ActionListener
	{
		public void actionPerformed (ActionEvent e)
		{
			
		}
	}
	
}
